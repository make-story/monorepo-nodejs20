{
  "name": "@makestory/e2e-test",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('use Yarn, no NPM')\"",
    "dev:server": "NODE_ENV=${NODE_ENV:=development} PORT=9040 PORT_SSL=443 node --loader ts-node/esm --inspect server.ts",
    "build:server": "tsc --project tsconfig.server.json",
    "start:server:dev": "NODE_ENV=development node _dist/server.js",
    "start:server:prd": "NODE_ENV=production node _dist/server.js"
  },
  "dependencies": {
    "@makestory/config": "*",
    "@makestory/event-manager": "*",
    "@makestory/fetch-manager": "*",
    "@makestory/logging-manager": "*",
    "@makestory/utils": "*",
    "@playwright/test": "^1.41.2",
    "@types/dotenv-flow": "^3.3.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.16",
    "axios": "1.6.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv-flow": "^4.1.0",
    "typescript": "5.1.3"
  },
  "devDependencies": {}
}
