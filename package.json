{
  "name": "product",
  "private": true,
  "packageManager": "yarn@1.22.19",
  "version": "1.0.0",
  "author": "",
  "license": "ISC",
  "description": "Node.js v20.10.0 | Next.js 14.0.3",
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "nohoist": []
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "yarn workspaces run lint --color",
    "typecheck": "yarn workspaces run typecheck",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "turbo:build": "turbo build",
    "turbo:dev": "turbo dev",
    "turbo:lint": "turbo lint",
    "storybook": "yarn workspace @ysm/storybook",
    "nextjs14": "yarn workspace @ysm/nextjs14",
    "dev": "concurrently \"yarn storybook dev\" \"yarn nextjs14 dev\""
  },
  "dependencies": {
    "autoprefixer": "^10.0.1",
    "concurrently": "^8.2.2",
    "eslint": "8.55.0",
    "eslint-config-next": "14.0.3",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-next": "^0.0.0",
    "postcss": "^8",
    "prettier": "3.1.1",
    "tsx": "^4.7.0",
    "turbo": "^1.11.2",
    "typescript": "5.1.3"
  },
  "devDependencies": {},
  "engines": {
    "node": "20.10.0"
  }
}
