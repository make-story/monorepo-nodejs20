{
  "extends": "@makestory/config/tsconfig.base.json",
  "compilerOptions": {
    "jsx": "preserve",
    "baseUrl": ".",
    "typeRoots": ["./node_modules/@types", "./@types"],
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      //"#/*": ["./src/*"]
      "@/*": ["./src/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", "**/*.d.ts", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
