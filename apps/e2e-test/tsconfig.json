{
  "extends": "@makestory/config/tsconfig.server.json",
  "compilerOptions": {
    "outDir": "dist",
    "allowJs": true,
    "checkJs": true,
    "paths": {
      "#/*": ["./*"],
      "@/*": ["./src/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", "**/*.d.ts"],
  "exclude": ["node_modules"]
}
