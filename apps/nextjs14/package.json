{
  "name": "@makestory/nextjs14",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('use Yarn, no NPM')\"",
    "dev": "NEXT_PRIVATE_LOCAL_WEBPACK=true NODE_OPTIONS='--inspect' next dev",
    "dev:turbo": "NODE_OPTIONS='--inspect' next dev --turbo",
    "dev:server": "NODE_ENV=${NODE_ENV:=development} PORT=9040 PORT_SSL=443 node --loader ts-node/esm --inspect server.ts",
    "build": "next build",
    "build:server": "tsc --project tsconfig.server.json",
    "start": "NODE_ENV=${NODE_ENV:=development}  next start",
    "start:dev": "NODE_ENV=development next start",
    "start:prd": "NODE_ENV=production next start",
    "start:server:dev": "NODE_ENV=development node _dist/server.js",
    "start:server:prd": "NODE_ENV=production node _dist/server.js",
    "lint": "next lint"
  },
  "dependencies": {
    "@module-federation/nextjs-mf": "^8.1.7",
    "@reduxjs/toolkit": "1.9.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/dotenv-flow": "^3.3.3",
    "@types/express": "^4.17.21",
    "@types/node": "20.3.1",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.5",
    "@types/redux-logger": "3.0.9",
    "@makestory/config": "*",
    "@makestory/event-manager": "*",
    "@makestory/fetch-manager": "*",
    "@makestory/logging-manager": "*",
    "@makestory/utils": "*",
    "axios": "1.6.5",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv-flow": "^4.1.0",
    "msw": "^2.1.5",
    "next": "14.0.3",
    "next-auth": "^4.24.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^9.0.1",
    "react-redux": "8.1.0",
    "redux-logger": "3.0.6",
    "redux-persist": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "styled-components": "^6.1.6",
    "tailwindcss": "^3.3.0",
    "typescript": "5.1.3",
    "use-query-params": "^2.2.1"
  },
  "devDependencies": {},
  "msw": {
    "workerDirectory": "public"
  }
}
